<article class="tile tile-onebyone">
	<a href="{{partner.href}}"  data-track="tech-shop-motorola+1x2">
		<div class="tile__hero">
			<img src="https://images1.unidays.world/perks/native/twoxone/036854d5-2ce9-4c6f-9b94-77c5cb33cc35" alt="Motorola">
		</div>
		<div class="tile__inner">
			<div class="tile__logo">
				<picture>
					<source srcset="{{partner.image}}" type="image/svg+xml">

					<img src="{{partner.image}}" alt="{{partner.alt}}">
				</picture>
			</div>
			{{> tile-discount partner.discount }}
		</div>
	</a>
</article>