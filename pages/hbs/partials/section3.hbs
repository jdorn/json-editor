

<article class="section3 {{colourClass}}">

	<div class="section-inner">

		<h2 class="title {{title.colourClass}}">
			{{title.text}}
		</h2>

		<div class="partners">
			<div class="scroller">
				<div class="tile__scroll-tile-onebyone">
					<div class="tile__scroll">


						{{#each partners}}
							{{> tile-onexone }}
						{{/each}}


					</div>
				</div>
			</div>

		</div>

		<div class="register">
			{{> button-link link }}
		</div>

	</div>

</article>
